import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int m=sc.nextInt();
        int[] arr1=new int[m];
        for(int i=0;i<m;i++){
            arr1[i]=sc.nextInt();
        }
        int n=sc.nextInt();
        int[] arr2=new int[n];
        for(int j=0;j<n;j++){
            arr2[j]=sc.nextInt();
        }
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++){
                if(arr1[i]==arr2[j]){
                     System.out.print(arr1[i]+" ");
                }    
            }
        }
    }
}
