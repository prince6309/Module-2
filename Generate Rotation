import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        String r =s+s;
        int i=s.length();
        int j=r.length()-1;
        while(i>0){
            for(int k=i;k<=j;k++){
                System.out.print(r.charAt(k));
            }
            System.out.println();
            i--;
            j--;
        }
        
    }
}
