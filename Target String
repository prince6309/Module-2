import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        String t=sc.next();
        s=s+s;
        if(s.indexOf(t)==-1){
            System.out.println("False");
            
        }
        else
            System.out.println("True");
        
    }
}
